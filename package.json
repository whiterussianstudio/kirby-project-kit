{
  "scripts": {
    "serve": "php -S 127.0.0.1:8080 -t public server.php",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "production": "mix --production",
    "start": "concurrently \"npm:watch\" \"npm:serve\""
  },
  "dependencies": {
    "@whiterussianstudio/tailwind-debug-mq": "^1.1.2",
    "@whiterussianstudio/tailwind-easing": "^1.0.0",
    "tailwindcss": "^2.2.4"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "laravel-mix": "^6.0.25"
  }
}
